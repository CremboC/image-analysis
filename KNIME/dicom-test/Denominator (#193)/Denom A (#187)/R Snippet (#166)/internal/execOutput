> rows <- knime.flow.in[["rows"]]
> 
> im <- as.matrix(knime.in[0 : rows, 0 : cols])
> x <- as.matrix(knime.in[0 : rows, (cols + 2) : (cols * 2 + 1)])
> 
> rez <- im * x ^ 2
> rez <- sum(rez)
> 
> knime.out <- data.frame(rez)
